{
  "main": "js/app.js",
  "scripts": {
    "start": "npm run build; npm run server",
    "server": "../node_modules/6to5/bin/6to5-node server.js",
    "build": "../node_modules/browserify/bin/cmd.js js/app.js > build/bundle.js",
    "watch": "../node_modules/watchify/bin/cmd.js . -o build/bundle.js -v -d"
  },
  "browserify": {
    "transform": [
      "6to5ify",
      "reactify",
      "envify"
    ]
  },
  "dependencies": {
    "alt": "^0.10.2",
    "es6-promise": "^2.0.1",
    "express": "^4.11.2",
    "object-assign": "^2.0.0",
    "react": "^0.12.2"
  },
  "devDependencies": {
    "6to5": "^3.3.4",
    "6to5ify": "^4.0.0",
    "browserify": "^8.0.3",
    "envify": "^3.2.0",
    "reactify": "^0.17.1",
    "uglify-js": "^2.4.16",
    "watchify": "^2.2.1"
  }
}
